{
    "name": "pmatseykanets/artisan-io",
    "description": "Artisan data import command for Laravel",
    "keywords": ["laravel", "data", "import", "delimited", "csv", "tsv"],
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Matseykanets",
            "email": "pmatseykanets@gmail.com",
            "homepage": "https://github.com/pmatseykanets",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1",
        "illuminate/container": "~5.1",
        "illuminate/database": "~5.1",
        "illuminate/validation": "~5.1",
        "illuminate/config": "~5.1",
        "illuminate/console": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "ArtisanIo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtisanIo\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtisanIo\\ArtisanIoServiceProvider"
            ]
        }
    }
}
